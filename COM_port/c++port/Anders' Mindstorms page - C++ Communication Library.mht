    connection<FONT color=3Dblack>-</FONT><FONT =
color=3Dblack>&gt;</FONT>connect<FONT color=3Dblack>(</FONT><FONT =
color=3Dmaroon>40</FONT><FONT color=3Dblack>)</FONT>;
    cout <FONT color=3Dblack>&lt;</FONT><FONT color=3Dblack>&lt;</FONT> =
<FONT color=3Dmaroon>"Connected"</FONT> <FONT =
